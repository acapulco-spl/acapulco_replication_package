<?xml version="1.0" encoding="UTF-8"?>
<mdeoptimiser4efm:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:mdeoptimiser4efm="http://www.example.org/mdeoptimiser4efm"
    name="WeaFQAsSimpleRootMandatoryHistory">
  <ownedRoot
      name="WeaFQAsSimpleRoot"
      optional="false">
    <ownedFeatures
        xsi:type="mdeoptimiser4efm:GroupFeature"
        name="WeaFQAsSimple"
        optional="false"
        childMinCardinality="1"
        childMaxCardinality="3">
      <ownedFeatures
          name="Persistence"/>
      <ownedFeatures
          xsi:type="mdeoptimiser4efm:GroupFeature"
          name="Security"
          childMinCardinality="1"
          childMaxCardinality="2">
        <ownedFeatures
            name="Auth">
          <ownedFeatures
              name="Signin"
              optional="false"/>
          <ownedFeatures
              xsi:type="mdeoptimiser4efm:GroupFeature"
              name="AuthMethod"
              optional="false"
              childMinCardinality="1"
              childMaxCardinality="1">
            <ownedFeatures
                name="Pass"/>
            <ownedFeatures
                name="ID"/>
          </ownedFeatures>
        </ownedFeatures>
        <ownedFeatures
            xsi:type="mdeoptimiser4efm:GroupFeature"
            name="Encr"
            childMinCardinality="1"
            childMaxCardinality="1">
          <ownedFeatures
              name="RSA"/>
          <ownedFeatures
              name="AES"/>
        </ownedFeatures>
      </ownedFeatures>
      <ownedFeatures
          xsi:type="mdeoptimiser4efm:GroupFeature"
          name="Usability"
          childMinCardinality="1"
          childMaxCardinality="2">
        <ownedFeatures
            name="Log">
          <ownedFeatures
              name="History"
              optional="false"/>
        </ownedFeatures>
        <ownedFeatures
            name="Feedback"/>
      </ownedFeatures>
    </ownedFeatures>
  </ownedRoot>
  <crossTreeConstraints
      leftFeature="History"
      rightFeature="Security"/>
</mdeoptimiser4efm:FeatureModel>
