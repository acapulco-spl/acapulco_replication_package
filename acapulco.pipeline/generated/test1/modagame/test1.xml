<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<feature_model name="FeatureIDE model">
    <feature_tree>
:r R(R)
	:o F1(F1)
		:g [1,*]
			: F2(F2)
			: F3(F3)
	:o F4(F4)
		:g [1,1]
			: F5(F5)
			: F6(F6)
			: F7(F7)
</feature_tree>
    <constraints>
C1:~F1  or  F4
C2:~F3  or  ~F5
</constraints>
</feature_model>
